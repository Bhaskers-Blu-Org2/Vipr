<?xml version="1.0" encoding="utf-8" ?>
<Parameters>
  <!-- Value of MetadataDocumentUri must be set to a valid service document uri or a local file path 
  eg : http://services.odata.org/Experimental/OData/OData.svc/ or File://C://Odata.edmx  -->
  <Parameter Name="MetadataDocumentUri" Value="..\Edmx\SharePoint.edmx.xml" />
  <!-- Namespace prefix will replace the original namespace, unless the model has several namespaces.-->
  <Parameter Name="NamespacePrefix" Value="Microsoft.Office365.SharePoint" />
  <!-- Value of TargetLanguage must be CSharp or VB -->
  <Parameter Name="TargetLanguage" Value="CSharp" />
  
  <Parameter Name="CapitalizeProperties" Value="True" />
  
  <Parameter Name="NamesToChange" Value="EntityContainer -> SharePointClient ; Microsoft.FileServices -> FileServices; Microsoft.CoreServices -> CoreServices; " />
</Parameters>
